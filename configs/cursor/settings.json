{
    "licenser.disableAutoHeaderInsertion": true,
    "editor.smoothScrolling": false,
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    },
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex -> bibtex -> pdflatex*2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        },
        {
            "name": "pdflatex",
            "tools": [
                "pdflatex"
            ]
        },
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "xelatex -> bibtex -> xelatex*2",
            "tools": [
                "xelatex",
                "bibtex",
                "xelatex",
                "xelatex"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.ist",
        "*.fls",
        "*.log",
        "*.fdb_latexmk"
    ],
    "latex-workshop.view.pdf.viewer": "external",
    "latex-workshop.view.pdf.external.viewer.args": [
        "0",
        "%PDF%"
    ],
    "latex-workshop.view.pdf.external.synctex.args": [
        "-forward-search",
        "%TEX%",
        "%LINE%",
        "-reuse-instance",
        "-inverse-search",
        "\"C:\\Users\\hyy\\AppData\\Local\\Programs\\Microsoft VS Code/Code.exe\" \"C:\\Users\\hyy\\AppData\\Local\\Programs\\Microsoft VS Code\\resources\\app\\out\\cli.js\" -r -g \"%f:%l\"",
        "%PDF%"
    ],
    "latex-workshop.synctex.afterBuild.enabled": true,
    "C_Cpp.clang_format_style": "Microsoft",
    "C_Cpp.updateChannel": "Insiders",
    "workbench.editorAssociations": {
        "*.svg": "default",
        "*.drawio": "hediet.vscode-drawio-text",
        "*.ipynb": "jupyter-notebook",
        "*.cls": "latex",
        "*.md": "default",
        "git-rebase-todo": "default"
    },
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "files.associations": {
        "*.ino": "cpp",
        "*.m": "matlab",
        "*.cls": "latex",
        "*.dtx": "latex",
        "*.lsf": "matlab"
    },
    "files.autoGuessEncoding": true,
    "editor.wordWrapColumn": 8,
    "editor.snippetSuggestions": "top",
    "settingsSync.ignoredSettings": [
        "latex-workshop.view.pdf.external.synctex.args",
        "terminal.integrated.fontFamily",
        "latex-workshop.view.pdf.external.viewer.command",
        "latex-workshop.view.pdf.external.synctex.command",
        "terminal.external.windowsExec",
        "python.languageServer"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "remote.SSH.remotePlatform": {
    },
    "diffEditor.wordWrap": "off",
    "screencastMode.fontSize": 30,
    "screencastMode.keyboardOverlayTimeout": 500,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "files.defaultLanguage": "P`",
    "editor.suggestSelection": "first",
    "gitlens.advanced.messages": {
        "suppressCreatePullRequestPrompt": true
    },
    "terminal.integrated.defaultProfile.windows": "Ubuntu-22.04 (WSL)",
    "notebook.output.textLineLimit": 10000,
    "terminal.integrated.inheritEnv": false,
    "remote.SSH.configFile": "~/.ssh/config",
    "settingsSync.ignoredExtensions": [
        "james-yu.latex-workshop",
        "vasilescur.better-mips",
        "tcwalther.cython",
        "metalcode-eu.darwin-riscv",
        "hediet.vscode-drawio-insiders-build",
        "wholroyd.jinja",
        "himanoa.python-autopep8",
        "ms-vscode-remote.remote-wsl",
        "ms-vscode-remote.remote-containers"
    ],
    "debug.inlineValues": false,
    "git.confirmSync": false,
    "editor.inlineSuggest.enabled": true,
    "cmake.configureOnOpen": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "editor.accessibilitySupport": "off",
    "terminal.integrated.customGlyphs": false,
    "editor.minimap.enabled": false,
    "git.ignoreRebaseWarning": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.scrollback": 100000,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "python.formatting.provider": "black",
    "terminal.integrated.env.osx": {
        "DISPLAY": "127.0.0.1:10.0"
    },
    "debug.allowBreakpointsEverywhere": true,
    "window.commandCenter": false,
    "latex-workshop.linting.chktex.exec.path": "/Library/TeX/texbin/chktex",
    "latex-workshop.latex.autoBuild.run": "onSave",
    "git.openRepositoryInParentFolders": "always",
    "[latex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.typeCheckingMode": "basic",
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
    "editor.fontFamily": "SauceCodePro Nerd Font",
    "[matlab]": {
        "editor.defaultFormatter": "MathWorks.language-matlab"
    },
    "remote.autoForwardPortsSource": "hybrid",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "gitlens.views.commitDetails.files.layout": "list",
    "explorer.confirmPasteNative": false,
    "workbench.editor.wrapTabs": true,
    "search.useParentIgnoreFiles": true,
    "cursor.cpp.disabledLanguages": [],
    "gitlens.codeLens.enabled": false,
    "workbench.colorTheme": "Default Dark+",
    "zenMode.hideLineNumbers": false,
    "zenMode.centerLayout": false,
    "terminal.integrated.stickyScroll.enabled": true,
    "notebook.stickyScroll.enabled": true,
    "editor.stickyScroll.defaultModel": "indentationModel",
    "debug.onTaskErrors": "abort",

}